<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="vboxElementos" alignment="CENTER" prefHeight="196.0" prefWidth="135.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controlador.ProductoBaseController">
   <children>
      <ImageView fx:id="ivimagen" fitHeight="99.0" fitWidth="90.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../IMAGES/vita%20c%20r.png" />
         </image>
      </ImageView>
      <Label fx:id="lblnombre" text="Vitamina c">
         <font>
            <Font name="Baskerville Old Face" size="21.0" />
         </font>
      </Label>
      <Label fx:id="lblprecio" text="\$2500">
         <font>
            <Font name="Arial Italic" size="19.0" />
         </font>
      </Label>
      <Button fx:id="btnver" mnemonicParsing="false" onAction="#ver" text="Ver" />
   </children>
</VBox>
